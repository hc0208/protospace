.jumbotron
  .container.text-center
    = image_tag("header_logo.png", alt: "PROTO TYPE - SHARE THE PROTOTYPE", height:"73px")

.filter-nav
  .container
    .row
      %ul.nav.nav-pills.col-lg-11
        - if current_page?(root_path)
          %li.active{role: "presentation"}
            = link_to "Popular PROTO", root_path
          %li{role: "presentation"}
            = link_to "NewestPROTO", newest_prototypes_path
        - else
          %li{role: "presentation"}
            = link_to "Popular PROTO", root_path
          %li.active{role: "presentation"}
            = link_to "NewestPROTO", newest_prototypes_path
      = button_tag "View Tags", class: "btn btn-default col-lg-1"

.container.proto-list
  .row
    = render partial: "prototype", collection: @prototype

.text-center
  %ul.pagination
    %li.disabled
      %a{"aria-label" => "Previous", :href => "#"}
        %span{"aria-hidden" => "true"} «
    %li.active
      %a{:href => "#"}
        1
        %span.sr-only (current)
    %li
      %a{:href => "#"} 2
    %li
      %a{:href => "#"} 3
    %li
      %a{:href => "#"} 4
    %li
      %a{:href => "#"} 5
    %li
      %a{"aria-label" => "Next", :href => "#"}
        %span{"aria-hidden" => "true"} »

